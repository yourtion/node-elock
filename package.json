{
  "name": "elock",
  "version": "0.2.0",
  "description": "Easy use distributed redis lock implementation",
  "main": "dist/lib/index.js",
  "typing": "dist/lib/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "test": "mocha --require ts-node/register src/test/**/*.test.*",
    "tag": "git tag v`node -p 'require(\"./package\").version'`",
    "lint": "tslint --project tsconfig.json --fix",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "doc": "typedoc src/lib/index.ts --out docs --mode file",
    "prepublish": "npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourtion/node-elock.git"
  },
  "keywords": [
    "redis",
    "lock"
  ],
  "author": "Yourtion <yourtion@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourtion/node-elock/issues"
  },
  "homepage": "https://github.com/yourtion/node-elock#readme",
  "dependencies": {
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/debug": "0.0.30",
    "@types/ioredis": "^3.2.5",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.41",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "source-map-support": "^0.4.15",
    "typedoc": "^0.10.0"
  }
}

{
  "name": "elock",
  "version": "1.0.0",
  "description": "Easy use distributed redis lock implementation",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/index.d.ts",
    "dist/index.js"
  ],
  "scripts": {
    "pretest": "npm run lint && tsc",
    "test": "mocha --exit --require ts-node/register src/test.ts",
    "test:cov": "export ISCOV=1 && nyc npm test -- --require source-map-support/register --full-trace --bail",
    "tag": "git tag v`node -p 'require(\"./package\").version'`",
    "lint": "tslint --project tsconfig.json --fix",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "doc": "typedoc src/index.ts --out docs --mode file",
    "prepublishOnly": "npm run clean && npm run test:cov",
    "postpublish": "npm run tag"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourtion/node-elock.git"
  },
  "keywords": [
    "redis",
    "lock"
  ],
  "author": "Yourtion <yourtion@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourtion/node-elock/issues"
  },
  "homepage": "https://github.com/yourtion/node-elock#readme",
  "dependencies": {
    "debug": "^3.1.0"
  },
  "peerDependencies": {
    "ioredis": "^3.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/debug": "0.0.30",
    "@types/ioredis": "^3.2.5",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "ioredis": "^3.2.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.2"
  }
}
